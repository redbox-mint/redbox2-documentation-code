###  http://purl.org/spar/fabio/Abstract

:Abstract rdf:type owl:Class ;
          
          rdfs:label "abstract"@en ;
          
          rdfs:subClassOf :Expression ;
          
          owl:disjointWith :StructuredSummary ;
          
          rdfs:comment "A brief summary of a work on a particular subject, designed to act as the point-of-entry that will help the reader quickly to obtain an overview of the work's contents.   The abstract may be an integral part of the work itself, written by the same author(s) and appearing at the beginning of a work such as a research paper, report, review or thesis.  Alternatively it may be separate from the published work itself, and written by someone other than the author(s) of the published work, for example by a member of a professional abstracting service such as CAB Abstracts."@en ;
          
          rdfs:seeAlso :StructuredSummary .



###  http://purl.org/spar/fabio/AcademicProceedings

:AcademicProceedings rdf:type owl:Class ;
                     
                     rdfs:label "academic proceedings"@en ;
                     
                     rdfs:subClassOf :ExpressionCollection ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty frbr:part ;
                                       owl:someValuesFrom :Expression
                                     ] ;
                     
                     rdfs:comment "A document containing the programme and collected papers, or their abstracts, presented at an academic meeting."@en .



###  http://purl.org/spar/fabio/Addendum

:Addendum rdf:type owl:Class ;
          
          rdfs:label "addendum"@en ;
          
          rdfs:subClassOf :Expression ;
          
          rdfs:comment "An item of material added at the end of a book or other publication, typically to include omitted or late-arriving material. "@en .



###  http://purl.org/spar/fabio/Algorithm

:Algorithm rdf:type owl:Class ;
           
           rdfs:label "algorithm"@en ;
           
           rdfs:subClassOf :Specification ;
           
           rdfs:comment "A precise sequential set of pre-defined logical rules or computational operations to be employed for solving a particular problem in a finite number of steps." .



###  http://purl.org/spar/fabio/AnalogItem

:AnalogItem rdf:type owl:Class ;
            
            rdfs:label "analog item"@en ;
            
            rdfs:subClassOf :Item ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty frbr:exemplarOf ;
                              owl:someValuesFrom :AnalogManifestation
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :isStoredOn ;
                              owl:someValuesFrom :AnalogStorageMedium
                            ] ;
            
            owl:disjointWith :DigitalItem ;
            
            rdfs:comment "A real object that is an exemplar of a fabio:Manifestation, such as a particular copy of the book 'Alice's adventures in Wonderland', that a person may own."@en ;
            
            dc:description "An analog item is an exemplar of an analog manifestation only and it is always stored in a storage medium suitable for analog objects, such as paper, vinyl discs and films."@en .



###  http://purl.org/spar/fabio/AnalogManifestation

:AnalogManifestation rdf:type owl:Class ;
                     
                     rdfs:label "analog manifestation"@en ;
                     
                     rdfs:subClassOf :Manifestation ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty frbr:exemplar ;
                                       owl:someValuesFrom :AnalogItem
                                     ] ;
                     
                     owl:disjointWith :DigitalManifestation ;
                     
                     rdfs:comment "A manifestation in an analog form."@en .



###  http://purl.org/spar/fabio/AnalogStorageMedium

:AnalogStorageMedium rdf:type owl:Class ;
                     
                     rdfs:label "analog storage medium"@en ;
                     
                     rdfs:subClassOf :StorageMedium ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :stores ;
                                       owl:allValuesFrom :AnalogItem
                                     ] ;
                     
                     rdfs:comment "A means of storing information in non-digital form, e.g. paper, film (for analogue photographs or movies), magnetic tape (for analogue sound recordings or video recordings) or vinyl disc."@en .



###  http://purl.org/spar/fabio/Announcement

