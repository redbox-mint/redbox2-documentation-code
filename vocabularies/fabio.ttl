###  http://purl.org/spar/fabio/NewspaperEditorial

:NewspaperEditorial rdf:type owl:Class ;
                    
                    rdfs:label "newspaper editorial"@en ;
                    
                    rdfs:subClassOf :Editorial ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty frbr:partOf ;
                                      owl:someValuesFrom :NewspaperIssue
                                    ] ;
                    
                    rdfs:comment "An editorial published in an issue of a newspaper."@en .



