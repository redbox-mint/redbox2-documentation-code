         rdfs:label "Dataset"@en-us ;
         
         rdfs:subClassOf :InformationResource ;
         
         vitro:shortDef "A named collection of data, usually containing only one type of data"^^xsd:string ;
         
         vitro:exampleAnnot "US Patent Data; US Job Data"^^xsd:string .



###  http://vivoweb.org/ontology/core#DateTimeInterval

:DateTimeInterval rdf:type owl:Class ;
                  
                  rdfs:label "Date/Time Interval"@en-us ;
                  
                  vitro:shortDef "a specific period or duration, defined by (optional) start and end date/times."^^xsd:string .



###  http://vivoweb.org/ontology/core#DateTimeValue

:DateTimeValue rdf:type owl:Class ;
               
               rdfs:label "Date/Time Value"@en-us ;
               
               vitro:shortDef "A date and/or time"^^xsd:string .



###  http://vivoweb.org/ontology/core#DateTimeValuePrecision

:DateTimeValuePrecision rdf:type owl:Class ;
                        
                        rdfs:label "Date/Time Value Precision"@en-us ;
                        
                        vitro:shortDef "Indicates the precision of the value of a DateTimeValue instance."^^xsd:string .



###  http://vivoweb.org/ontology/core#Department

:Department rdf:type owl:Class ;
            
            rdfs:label "Department"@en-us ;
            
            rdfs:subClassOf foaf:Organization ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :administers ;
                              owl:allValuesFrom :Grant
                            ] ;
            
            vitro:shortDef "A distinct, usually ongoing and specialized unit within a large organization."^^xsd:string ;
            
            vitro:descriptionAnnot "Definition modified from the definition here: http://dictionary.reference.com/browse/department. It is difficult to tell the difference between and department and a division."^^xsd:string ;
            
            vitro:exampleAnnot "Legal (department within a company); Use for any non-academic department"^^xsd:string .



###  http://vivoweb.org/ontology/core#Division

:Division rdf:type owl:Class ;
          
          rdfs:label "Division"@en-us ;
          
          rdfs:subClassOf foaf:Organization ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :administers ;
                            owl:allValuesFrom :Grant
                          ] ;
          
          vitro:shortDef "An area of corporate activity organized as an administrative or functional unit."^^xsd:string ;
          
          vitro:exampleAnnot "Cardiovascular Medicine (division within medicine)"^^xsd:string ;
          
          vitro:descriptionAnnot "Definition modified from http://www.thefreedictionary.com/division.  It is difficult to tell the difference between a division and a department."^^xsd:string .



###  http://vivoweb.org/ontology/core#EditorRole

:EditorRole rdf:type owl:Class ;
            
            rdfs:label "Editor Role"@en-us ;
            
            rdfs:subClassOf :Role ;
            
            vitro:shortDef "An ongoing editorial responsibility for a bibo:Collection, such as a Journal or Series"^^xsd:string ;
            
            vitro:descriptionAnnot "An ongoing editorial responsibility for a bibo:Collection, such as a Journal or Series"^^xsd:string .



###  http://vivoweb.org/ontology/core#EditorialArticle

:EditorialArticle rdf:type owl:Class ;
                  
                  rdfs:label "Editorial Article"@en-us ;
                  
                  rdfs:subClassOf bibo:Article ;
                  
                  vitro:shortDef "An article of opinion, typically published in a newspaper. For academics, most commonly Op Ed pieces"^^xsd:string .



###  http://vivoweb.org/ontology/core#EducationalTraining

:EducationalTraining rdf:type owl:Class ;
                     
                     rdfs:label "Educational Training"@en-us ;
                     
                     rdfs:subClassOf [ rdf:type owl:Restriction ;
                                       owl:onProperty :supplementalInformation ;
                                       owl:allValuesFrom xsd:string
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :dateTimeInterval ;
                                       owl:allValuesFrom :DateTimeInterval
                                     ] ;
                     
                     vitro:exampleAnnot "Currently any abstract name is given to individuals of this class. This could change in the future."^^xsd:string ;
                     
                     vitro:shortDef "Represents educational training that has been received."^^xsd:string ;
                     
                     vitro:descriptionAnnot "This connects person to their academic degree through this educational training, but can also be used when the training does not result in a degree."^^xsd:string .



###  http://vivoweb.org/ontology/core#EmeritusFaculty

:EmeritusFaculty rdf:type owl:Class ;
                 
                 rdfs:label "Faculty Member Emeritus"@en-us ;
                 
                 rdfs:subClassOf foaf:Person ;
                 
                 vitro:shortDef "A retired faculty member who has retained their rank, title and privileges."^^xsd:string .



###  http://vivoweb.org/ontology/core#EmeritusLibrarian

:EmeritusLibrarian rdf:type owl:Class ;
                   
                   rdfs:label "Librarian Emeritus"@en-us ;
                   
                   rdfs:subClassOf foaf:Person ;
                   
                   vitro:shortDef "A retired librarian who has retained their rank, title and privileges."^^xsd:string .



###  http://vivoweb.org/ontology/core#EmeritusProfessor

:EmeritusProfessor rdf:type owl:Class ;
                   
                   rdfs:label "Professor Emeritus"@en-us ;
                   
                   rdfs:subClassOf :EmeritusFaculty ,
                                   foaf:Person ;
                   
                   vitro:shortDef "A retired professor who has retained their rank, title and privileges."^^xsd:string .



###  http://vivoweb.org/ontology/core#Equipment

:Equipment rdf:type owl:Class ;
           
           rdfs:label "Equipment"@en-us ;
           
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :freetextKeyword ;
                             owl:allValuesFrom rdfs:Literal
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :webpage ;
                             owl:allValuesFrom :URLLink
                           ] ;
           
           vitro:descriptionAnnot "A network server is one example. Medical schools and research laboratories can list professional equipment, such as microscopes."^^xsd:string ;
           
           vitro:shortDef "A physical object provided for specific purpose, task or occupation. "^^xsd:string ;
           
           vitro:exampleAnnot "server; Bruker Vector-33 FT-IR"^^xsd:string .



###  http://vivoweb.org/ontology/core#EventSeries

:EventSeries rdf:type owl:Class ;
             
             rdfs:label "Event Series"@en-us ;
             
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :domesticGeographicFocus ;
                               owl:allValuesFrom :GeographicRegion
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :geographicFocus ;
                               owl:allValuesFrom :GeographicRegion
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :realizedRole ;
                               owl:allValuesFrom :Role
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :description ;
                               owl:allValuesFrom rdfs:Literal
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :dateTimeInterval ;
                               owl:allValuesFrom :DateTimeInterval
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :internationalGeographicFocus ;
                               owl:allValuesFrom :GeographicRegion
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasSubjectArea ;
                               owl:allValuesFrom owl:Thing
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :contactInformation ;
                               owl:allValuesFrom rdfs:Literal
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :webpage ;
                               owl:allValuesFrom :URLLink
                             ] ;
             
             vitro:descriptionAnnot "A generic class which may include a conference series, a course section, a seminar series, or a workshop series. When possible, use one of these more specific classes."^^xsd:string ;
             
             vitro:exampleAnnot "Only use if no specific subclasses of core:EventSeries desribe the activity."^^xsd:string ;
             
             vitro:shortDef "Two or more events that occur at different times and are connected to each other."^^xsd:string .



###  http://vivoweb.org/ontology/core#Exhibit

:Exhibit rdf:type owl:Class ;
         
         rdfs:label "Exhibit"@en-us ;
         
         rdfs:subClassOf event:Event ;
         
         vitro:shortDef "The showing of an object or a collection of objects, in an organized manner."^^xsd:string .



###  http://vivoweb.org/ontology/core#ExtensionUnit

:ExtensionUnit rdf:type owl:Class ;
               
               rdfs:label "Extension Unit"@en-us ;
               
               rdfs:subClassOf foaf:Organization ;
               
               vitro:shortDef "A unit devoted primarily to extension activities, whether for outreach or research."^^xsd:string ;
               
               vitro:exampleAnnot "Alachua County Extension Office"^^xsd:string .



###  http://vivoweb.org/ontology/core#F1000Link

:F1000Link rdf:type owl:Class ;
           
           rdfs:label "F1000 Link"@en-us ;
           
           rdfs:subClassOf :URLLink ;
           
           vitro:descriptionAnnot "F1000, is a place where faculty go to critique papers published in PubMed. Any given record in F1000 might have anywhere from one to dozens of reviews."^^xsd:string .



###  http://vivoweb.org/ontology/core#Facility

:Facility rdf:type owl:Class ;
          
          rdfs:label "Facility"@en-us ;
          
          rdfs:subClassOf :GeographicLocation ;
          
          vitro:descriptionAnnot "Distinct from the organization that runs it; e.g., a laboratory may be an organization but may be run by another organization and only consist of facilities housing equipment or services. Can be a building or place that provides a particular service or is used for a particular activity. Use the specific Building or Room whenever possible. Short definition from http://dictionary.reference.com/browse/facility."^^xsd:string ;
          
          vitro:shortDef "Something designed, built, installed, etc., to serve a specific function or activity affording a convenience or service."^^xsd:string ;
          
          vitro:exampleAnnot "Use subclasses of core:Facility subclasses instead of this class if possible"^^xsd:string .



###  http://vivoweb.org/ontology/core#FacultyAdministrativePosition

:FacultyAdministrativePosition rdf:type owl:Class ;
                               
                               rdfs:label "Faculty Administrative Position"@en-us ;
                               
                               rdfs:subClassOf :Position ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :positionForPerson ;
                                                 owl:allValuesFrom :FacultyMember
                                               ] ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :positionInOrganization ;
                                                 owl:allValuesFrom foaf:Organization
                                               ] ;
                               
                               vitro:shortDef "An academic administrative position (associate dean, etc.) as distinct from a professional administrative position (non-academic)"^^xsd:string ;
                               
                               vitro:exampleAnnot "Associate Dean"^^xsd:string ;
                               
                               vitro:descriptionAnnot "That is a position held by an academic faculty member who works for administration."^^xsd:string .



###  http://vivoweb.org/ontology/core#FacultyMember

:FacultyMember rdf:type owl:Class ;
               
               rdfs:label "Faculty Member"@en-us ;
               
               rdfs:subClassOf foaf:Person ;
               
               vitro:shortDef "A person with at least one academic appointment to a specific faculty of a university or institution of higher learning."^^xsd:string ;
               
               vitro:descriptionAnnot "Definition from here: http://research.carleton.ca/htr/defs.php."^^xsd:string .



###  http://vivoweb.org/ontology/core#FacultyMentoringRelationship

:FacultyMentoringRelationship rdf:type owl:Class ;
                              
                              rdfs:label "Faculty Mentoring Relationship"@en-us ;
                              
                              rdfs:subClassOf :AdvisingRelationship ;
                              
                              vitro:shortDef "An advisory relationship in which one faculty member mentors another faculty member."^^xsd:string .



###  http://vivoweb.org/ontology/core#FacultyPosition

:FacultyPosition rdf:type owl:Class ;
                 
                 rdfs:label "Faculty Position"@en-us ;
                 
                 rdfs:subClassOf :Position ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :positionInOrganization ;
                                   owl:allValuesFrom foaf:Organization
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :positionForPerson ;
                                   owl:allValuesFrom :FacultyMember
                                 ] ;
                 
                 vitro:shortDef "Academic position in a university or institution"^^xsd:string ;
                 
                 vitro:descriptionAnnot "Professor, associate professor and assistant professor are common positions for academic faculty."^^xsd:string ;
                 
                 vitro:exampleAnnot "Professor; Associate Professor; Assistant Professor"^^xsd:string .



###  http://vivoweb.org/ontology/core#Foundation

:Foundation rdf:type owl:Class ;
            
            rdfs:label "Foundation"@en-us ;
            
            rdfs:subClassOf foaf:Organization ;
            
            vitro:shortDef "An institution financed by a donation or legacy to aid research, education, the arts, etc. "^^xsd:string ;
            
            vitro:descriptionAnnot "Definition take from: http://dictionary.reference.com/browse/foundation."^^xsd:string ;
            
            vitro:exampleAnnot "The Ford Foundation"^^xsd:string .



###  http://vivoweb.org/ontology/core#FundingOrganization

:FundingOrganization rdf:type owl:Class ;
                     
                     rdfs:label "Funding Organization"@en-us ;
                     
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :awardsGrant ;
                                           owl:someValuesFrom :Grant
                                         ] ;
                     
                     rdfs:subClassOf foaf:Organization ;
                     
                     vitro:shortDef "A defined class of organizations that fund Grants."^^xsd:string ;
                     
                     vitro:exampleAnnot "National Institute of Health (NIH)"^^xsd:string .



###  http://vivoweb.org/ontology/core#GeographicLocation

:GeographicLocation rdf:type owl:Class ;
                    
                    rdfs:label "Geographic Location"@en-us ;
                    
                    rdfs:subClassOf :Location ;
                    
                    vitro:shortDef "A location having coordinates in geographic space."^^xsd:string ;
                    
                    vitro:descriptionAnnot "Removed the word \"stable\" because disputed territories from geopolitical.owl are included. This could imply that the geographic coordinates could change. I've also copied this definition to core:Geographic Location.  I think core:Geographic Location and core:Geographic Region are both the same and only one is needed. There is also geopolitical.owl:geographical_region which further causes confusion."^^xsd:string ;
                    
                    vitro:exampleAnnot "Use subclasses of core:Geographic Location subclasses instead of this class if possible."^^xsd:string .



###  http://vivoweb.org/ontology/core#GeographicRegion

:GeographicRegion rdf:type owl:Class ;
                  
                  rdfs:label "Geographic Region"@en-us ;
                  
                  rdfs:subClassOf :GeographicLocation ;
                  
                  vitro:shortDef "A location having coordinates in geographic space."^^xsd:string ;
                  
                  vitro:descriptionAnnot "Removed the word \"stable\" because disputed territories from geopolitical.owl are included. This could imply that the geographic coordinates could change. This definition was originally in core:Geographic Location.  I simply copied the definition from there.  I think core:Geographic Location and core:Geographic Region are both the same and only one is needed. There is also geopolitical.owl:geographical_region which further causes confusion."^^xsd:string ;
                  
                  vitro:exampleAnnot "Use subclasses of core:Geographic Region subclasses instead of this class if possible."^^xsd:string .



###  http://vivoweb.org/ontology/core#GeopoliticalEntity

:GeopoliticalEntity rdf:type owl:Class ;
                    
                    rdfs:label "Geopolitical Entity"@en-us ;
                    
                    rdfs:subClassOf :GeographicRegion ;
                    
                    vitro:shortDef "A geographical area which is associated with some sort of political  structure."^^xsd:string ;
                    
                    vitro:descriptionAnnot "Short definition obtained here: http://en.wiktionary.org/wiki/geopolitical_entity."^^xsd:string ;
                    
                    vitro:exampleAnnot "Use subclasses of core:GeopoliticalEntity subclasses instead of this class if possible."^^xsd:string .



###  http://vivoweb.org/ontology/core#GovernmentAgency

:GovernmentAgency rdf:type owl:Class ;
                  
                  rdfs:label "Government Agency"@en-us ;
                  
                  rdfs:subClassOf foaf:Organization ;
                  
                  vitro:shortDef "A government agency is a permanent or semi-permanent organization in the machinery of government that is responsible for the oversight and administration of specific functions."^^xsd:string ;
                  
                  vitro:descriptionAnnot "Definition take from: http://en.wikipedia.org/wiki/Government_agency."^^xsd:string ;
                  
                  vitro:exampleAnnot "United States Library of Congress"^^xsd:string .



###  http://vivoweb.org/ontology/core#GraduateAdvisingRelationship

:GraduateAdvisingRelationship rdf:type owl:Class ;
                              
                              rdfs:label "Graduate Advising Relationship"@en-us ;
                              
                              rdfs:subClassOf :AdvisingRelationship ;
                              
                              vitro:shortDef "An advisory relationship in which a professor advises a graduate student."^^xsd:string .



###  http://vivoweb.org/ontology/core#GraduateStudent

:GraduateStudent rdf:type owl:Class ;
                 
                 rdfs:label "Graduate Student"@en-us ;
                 
                 rdfs:subClassOf :Student ;
                 
                 vitro:shortDef "A person who has already received a bachelor's degree and is working toward a Master's or Doctoral degree."^^xsd:string .



###  http://vivoweb.org/ontology/core#Grant

:Grant rdf:type owl:Class ;
       
       rdfs:label "Grant"@en-us ;
       
       rdfs:subClassOf :Agreement ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :supportedInformationResource ;
                         owl:allValuesFrom :InformationResource
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty bibo:abstract ;
                         owl:allValuesFrom rdfs:Literal
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :webpage ;
                         owl:allValuesFrom :URLLink
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :grantAwardedBy ;
                         owl:allValuesFrom foaf:Organization
                       ] ;
       
       vitro:exampleAnnot "An intramural or extramural award to support scholarly work, such as UF09179 (VIVO)"^^xsd:string ;
       
       vitro:shortDef "Financial assistance mechanism providing money, property, or both to an eligible entity to carry out an approved project or activity"^^xsd:string ;
       
       vitro:descriptionAnnot "Short definition is from the Glossary of NIH Terms."^^xsd:string .



###  http://vivoweb.org/ontology/core#Hospital

