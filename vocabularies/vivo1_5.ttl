###  http://vivoweb.org/ontology/core#Authorship

:Authorship rdf:type owl:Class ;
            
            rdfs:label "Authorship"@en-us ;
            
            rdfs:subClassOf :Relationship ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :linkedAuthor ;
                              owl:allValuesFrom foaf:Agent
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hideFromDisplay ;
                              owl:allValuesFrom xsd:boolean
                            ] ;
            
            rdfs:comment ""^^xsd:string ;
            
            vitro:descriptionAnnot """Authorship of journal articles, books and other original works is a means by which academics communicate the results of their scholarly work, establish priority for their discoveries, and build their reputation among their peers.
This class allows for linking an author to a publication while indicating information about that author's authorship."""^^xsd:string ;
            
            vitro:shortDef "Contains the authors name, their rank in the publication, and whether or not they are a corresponding author on the publication."^^xsd:string ;
            
            vitro:exampleAnnot "Currently any abstract name is given to members of this class. This could change in the future."^^xsd:string .



