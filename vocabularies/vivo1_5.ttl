          rdfs:subClassOf foaf:Organization ;
          
          vitro:shortDef "An institution that provides medical, surgical, or psychiatric care and treatment for the sick or the injured."^^xsd:string ;
          
          vitro:descriptionAnnot "Definition take from: http://dictionary.reference.com/browse/hospital."^^xsd:string ;
          
          vitro:exampleAnnot "Shands at the University of Florida"^^xsd:string .



