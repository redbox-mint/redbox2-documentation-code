###  http://purl.org/NET/c4dm/event.owl#agent

event:agent rdf:type owl:ObjectProperty ;
            
            rdfs:label "agent" ;
            
            vs:term_status "stable" ;
            
            rdfs:comment " Relates an event to an active agent (a person, a computer, ... ) " ;
            
            rdfs:domain event:Event ;
            
            owl:inverseOf event:isAgentIn ;
            
            rdfs:range foaf:Agent .



###  http://purl.org/NET/c4dm/event.owl#isAgentIn

event:isAgentIn rdf:type owl:ObjectProperty .



###  http://purl.org/NET/c4dm/event.owl#place

event:place rdf:type owl:FunctionalProperty ,
                     owl:ObjectProperty ;
            
            rdfs:label "place" ;
            
            rdfs:comment " Relates an event to a spatial object. " ;
            
            vs:term_status "stable" ;
            
            rdfs:domain event:Event ;
            
            rdfs:range geo:SpatialThing .



###  http://purl.org/NET/c4dm/event.owl#producedIn

event:producedIn rdf:type owl:ObjectProperty .



###  http://purl.org/NET/c4dm/event.owl#produced_in

event:produced_in rdf:type owl:ObjectProperty .



###  http://purl.org/NET/c4dm/event.owl#product

event:product rdf:type owl:ObjectProperty ;
              
              rdfs:label "product" ;
              
              vs:term_status "stable" ;
              
              rdfs:comment " Relates an event to something produced during the event---a sound, a pie, whatever... " ;
              
              rdfs:domain event:Event ;
              
              owl:inverseOf event:producedIn .



###  http://purl.org/NET/c4dm/event.owl#sub_event

event:sub_event rdf:type owl:ObjectProperty ;
                
                rdfs:label "sub-event" ;
                
                vs:term_status "stable" ;
                
                rdfs:comment " This property provides a way to split a complex event (for example, a performance involving several musicians) into simpler ones (one event per musician)." ;
                
                rdfs:range event:Event ;
                
                rdfs:domain event:Event .



###  http://purl.org/NET/c4dm/event.owl#time

event:time rdf:type owl:FunctionalProperty ,
                    owl:ObjectProperty ;
           
           rdfs:label "time" ;
           
           rdfs:comment "Relates an event to a time object, classifying a time region (either instantaneous or having an extent). By using the Timeline ontology here, you can define event happening on a recorded track or on any media with a temporal extent." ;
           
           vs:term_status "stable" ;
           
           rdfs:domain event:Event ;
           
           rdfs:range time:TemporalEntity .



